{
  "name": "vremel",
  "version": "0.1.0",
  "description": "JavaScript date utility library for Temporal",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json && babel dist/esm -d dist/cjs --out-file-extension .cjs",
    "test": "vitest",
    "typecheck": "tsc",
    "repl": "vite-node script/repl.ts"
  },
  "homepage": "https://github.com/fabon-f/vremel#readme",
  "bugs": {
    "url": "https://github.com/fabon-f/vremel/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabon-f/vremel.git"
  },
  "keywords": [
    "Temporal"
  ],
  "author": "fabon <syobon.hinata.public@gmail.com> (https://www.fabon.info/)",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/plugin-transform-modules-commonjs": "^7.23.3",
    "@js-temporal/polyfill": "^0.4.4",
    "@tsconfig/strictest": "^2.0.2",
    "@types/node": "^20.11.6",
    "babel-plugin-replace-import-extension": "^1.1.4",
    "consola": "^3.2.3",
    "rimraf": "^5.0.5",
    "temporal-polyfill": "^0.2.0",
    "typescript": "^5.3.3",
    "vite-node": "^1.2.1",
    "vitest": "^1.2.1"
  }
}
